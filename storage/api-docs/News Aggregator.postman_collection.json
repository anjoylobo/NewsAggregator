				{
					"name": "Login User",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Store token in environment\", function () {",
									"    var jsonData = pm.response.json(); // Parse the response body as JSON",
									"    if (jsonData.data && jsonData.data.token) {",
									"        pm.environment.set(\"authToken\", jsonData.data.token); // Save token in environment",
									"        console.log(\"Token stored:\", jsonData.data.token);",
									"    } else {",
									"        pm.test(\"Token not found in the response\");",
									"    }",
									"});",
									""
